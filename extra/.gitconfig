        email = martini.matus@gmail.com
[core]
        quotepath = false
        whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
[diff]
	tool = vimdiff
[color]
        ui = true
[color "branch"]
        current = yellow black
        local = yellow
        remote = magenta
[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red reverse
        new = green reverse
        whitespace = white reverse
[color "status"]
        added = yellow
        changed = green
        untracked = cyan reverse
        branch = magenta
[difftool]
	prompt = false
[alias]
	d = difftool
        ci = commit
        co = checkout
        br = branch
        st = status
        ls = ls-config
    d2 = difftool br_GFS master
